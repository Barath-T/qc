   ],
   "source": [
    "qc = QuantumCircuit(4)\n",
    "\n",
    "qc.h([0, 1, 3])\n",
    "\n",
    "qc.barrier()\n",
    "\n",
    "qc.cx(0, 1)\n",
    "qc.cx(1, 2)\n",
    "qc.cx(2, 3)\n",
    "\n",
    "qc.rz(pi/3, 3)\n",
    "\n",
    "qc.cx(2, 3)\n",
    "qc.cx(1, 2)\n",
    "qc.cx(0, 1)\n",
    "\n",
    "qc.barrier()\n",
    "\n",
    "qc.h([0, 1, 3])"
   ]
